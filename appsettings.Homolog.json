{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GlobalSettings": {
    "Selfhosted": true,
    "UniqueDeployed": false,
    "ProjectName": "DotNetEcosystemStudy-Homolog",
    "EnableCloudCommunication": false,
    "BaseServiceUri": {
      "CloudRegion": "homolog-eu",
      "Vault": "https://vault.homolog.example.com",
      "VaultWithHash": "homolog-eu-west-1",
      "VaultWithHashAndSecretManagerProduct": "homolog-eu-central-1",
      "Identity": "https://identity.homolog.example.com",
      "Admin": "https://admin.homolog.example.com",
      "Notifications": "https://notifications.homolog.example.com",
      "Sso": "https://sso.homolog.example.com",
      "Scim": "https://scim.homolog.example.com",
      "InternalNotifications": "https://internal-notifications.homolog.example.com",
      "InternalAdmin": "https://internal-admin.homolog.example.com",
      "InternalIdentity": "https://internal-identity.homolog.example.com",
      "InternalApi": "https://internal-api.homolog.example.com",
      "InternalVault": "https://internal-vault.homolog.example.com",
      "InternalSso": "https://internal-sso.homolog.example.com",
      "InternalScim": "https://internal-scim.homolog.example.com",
      "InternalBilling": "https://internal-billing.homolog.example.com"
    },
    "EventLogging": {
      "AzureServiceBus": {
        "ConnectionString": "Endpoint=sb://homolog-namespace.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=homologkey",
        "TopicName": "homolog-topic"
      },
      "RabbitMq": {
        "Hostname": "homolog-rabbitmq.cloudapp.azure.com",
        "Username": "homologuser",
        "Password": "homologsecurepassword123",
        "ExchangeName": "homolog-exchange"
      }
    }
  }
}